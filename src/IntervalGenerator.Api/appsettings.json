{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "Authentication": {
      "Enabled": true,
      "ApiKey": "demo-api-key",
      "ApiPassword": "demo-api-password"
